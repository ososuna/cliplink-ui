{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/auth\\/reset-password\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/my-account\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/short\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		}
	]
}